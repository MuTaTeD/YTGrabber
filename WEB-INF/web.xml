<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>YTGrabber</servlet-name>
        <servlet-class>me.uni.sushilkumar.ytgrabber.YTGrabber</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>YTDownload</servlet-name>
        <servlet-class>me.uni.sushilkumar.ytgrabber.YTDownload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>YTGrabber</servlet-name>
        <url-pattern>/YTGrabber</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>YTDownload</servlet-name>
        <url-pattern>/YTDownload</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
